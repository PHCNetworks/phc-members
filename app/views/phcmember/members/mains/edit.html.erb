<!-- Bread Crumbs -->
<div class="row wrapper border-bottom white-bg page-heading">
	<div class="col-lg-10">
		<h2>Member's Information</h2>
		<ol class="breadcrumb">
			<li>
				<%= link_to mtdevise.account_root_path, class: "nav-link" do %>
					Home
				<% end %>
			</li>
			<li>
				<%= link_to main_app.members_mains_path, class: "nav-link" do %>
					Members List
				<% end %>
			</li>
			<li class="active">
				Update <%= @members_main.mcfirstname + ' ' + @members_main.mclastname %> Member Information
			</li>
		</ol>
	</div>
	<div class="col-lg-2">
	</div>
</div>

<!-- Main Content -->
<div class="wrapper wrapper-content">
	<div class="panel">
		<div class="panel-heading">
			<h2><i class="fa fa-lightbulb-o"></i> Member Update</h2> <p>- Update vital information for <%= @members_main.mcfirstname + ' ' + @members_main.mclastname %>.</p>
		</div>
		<div class="panel-body">
			<%= render 'form' %>
		</div>
	</div>
</div>
