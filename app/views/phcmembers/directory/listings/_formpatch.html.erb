<%= form_for([@directory_listing.category, @directory_listing], url: directory_category_listing_path, method: :patch) do |f| %>
	
	<!-- PHC-Notifi Render Validation -->
	<%= render 'phcnotifi/validations', :object => @directory_listing %>

	<div class="form-group field_with_errors">
		<label>Select a Listing</label>
		<%= collection_select(:directory_listing, :business_id, Phcmembers::Members::Business.order('mbcompanyname'), :id, :mbcompanyname, {}, {class: "form-control"}) %>
	</div>

	<div class="actions">
		<%= f.submit "Submit", class: "btn blue-hoki" %>
	</div>

<% end %>
