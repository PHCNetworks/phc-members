<div class="row">
	<div class="col-md-6">
		<div class="panel panel-default">
			<div class="panel-heading">
				Category List
			</div>
			<div class="panel-body">
				<%= render 'phcmembers/directory/categories/components/category_list' %>
			</div>
			<div class="panel-footer">
				<%= link_to 'Update Category Name', edit_directory_category_path(@directory_category), class: "btn btn-primary btn-sm" %>
			</div>
		</div>
	</div>
	<div class="col-md-6">
		<div class="panel panel-default">
			<div class="panel-heading">
				Category Change Logs
			</div>
			<div class="panel-body">
				<%= render 'phcmembers/directory/categories/components/category_audits' %>
			</div>
		</div>
	</div>
</div>
