<p id="notice"><%= notice %></p>

<h1>Members Contacts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members_contacts.each do |members_contact| %>
      <tr>
        <td><%= link_to 'Show', members_contact %></td>
        <td><%= link_to 'Edit', edit_members_contact_path(members_contact) %></td>
        <td><%= link_to 'Destroy', members_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Members Contact', new_members_contact_path %>
