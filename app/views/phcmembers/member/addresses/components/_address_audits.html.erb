<div class="scroller" style="height: 320px;" data-always-visible="1" data-rail-visible1="0" data-handle-color="#D7DCE2">

	<ul class="list-group feeds">
		<% @member_address_versions.each do |member_address_version| %>
			<li class="list-group-item">
				<i class="fa fa-bell-o fa-fw"></i> <strong><%= member_address_version.event.capitalize %></strong> - <%= @member_address.mcaddressl1 + ' ' + @member_address.mccity  + ' ' + @member_address.mcprovince %> Listing on <i><%= l(member_address_version.created_at, format: "%-d/%m/%Y") %></i> at <i><%= l(member_address_version.created_at, format: "%H:%M %Z") %></i>
			</li>
		<% end %>
	</ul>

</div>
