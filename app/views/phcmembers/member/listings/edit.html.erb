<!-- Title System -->
<% phc_title "PHCMembers - Member Listing Manager" %>
<% phc_title_tagline "Update Listing Information for " + @member_profile.mfirstname + " " + @member_profile.mlastname %>

<!-- BreadCrumbs -->
<div class="row">
	<div class="col-lg-12">

		<!-- Bread Crumb -->
		<ol class="breadcrumb">
			<li><%= link_to "Members Home", member_root_path %></li>
			<li><%= link_to "Member Listings", member_profile_listings_path %></li>
			<li class="active"><%= yield(:phc_title_tagline) %></li>
		</ol>

	</div>
</div>

<!-- Page Title -->
<div class="row">
	<div class="col-lg-12">

		<h3 class="page-title">
			<%= yield(:phc_title) %> </br>
			<small><%= yield(:phc_title_tagline) %></small>
		</h3>

	</div>
</div>

<!-- Main Content -->
<div class="row">
	<div class="col-lg-12">

		<div class="card">
			<div class="card-header">
				<%= yield(:phc_title_tagline) %>
			</div>
			<div class="card-block">
				<%= render 'form' %>
			</div>
		</div>

	</div>
</div>
