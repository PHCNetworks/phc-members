<div class="scroller" style="height: 320px;" data-always-visible="1" data-rail-visible1="0" data-handle-color="#D7DCE2">

	<ul class="list-group feeds">
		<% @member_listing_versions.each do |member_listing_version| %>
			<li class="list-group-item">
				<i class="fa fa-bell-o fa-fw"></i> <strong><%= member_listing_version.event.capitalize %></strong> - <%= @member_listing.mbcompanyname %> Listing on <i><%= l(member_listing_version.created_at, format: "%-d/%m/%Y") %></i> at <i><%= l(member_listing_version.created_at, format: "%H:%M %Z") %></i>
			</li>
		<% end %>
	</ul>

</div>
